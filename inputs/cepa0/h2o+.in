molecule {
  1 2
  O 
  H 1 0.96
  H 1 0.96 2 104.5
}
set e_convergence 13
set d_convergence 13
set maxiter       100
set basis         sto-3g
set reference     uhf
set scf_type      pk
Escf  = energy('scf')

