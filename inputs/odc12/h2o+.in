molecule {
  1 2
  O 
  H 1 0.96
  H 1 0.96 2 104.5
}
set e_convergence   13
set d_convergence   13
set r_convergence   13
set maxiter         100
set basis           dzp
set reference       uhf
set scf_type        pk
set dcft_functional odc-12
energy('scf')

# call my python module
# note: you need meinsum/ in your PYTHONPATH to run this
from theory.odc12 import SpinOrbODC12

scfwfn = psi4.wavefunction()
mints  = psi4.MintsHelper()
odc12  = SpinOrbODC12(scfwfn, mints)
odc12.odc12_energy()

psi4.print_out("\n@ODC12   {:20.15f}".format(energy('dcft')))
